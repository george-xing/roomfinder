<div class="container">
  <div class="row">
    <h3>Listings</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @listings.each do |listing| %>
            <tr>
              <%= render listing %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
